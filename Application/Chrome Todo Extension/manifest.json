{
	"manifest_version": 3,
	"name": "To-Do List",
	"version": "1.0",
	"description": "A simple and clean to-do list extension!",
	"action": {
		"default_popup": "index.html",
		"default_icons": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"css": ["style.css"],
			"js": ["javascript.js"],
			"matches": ["http://*.example.com//"]
		}
	],
	"permissions": ["storage"],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"128": "icons/icon128.png"
	}
}
