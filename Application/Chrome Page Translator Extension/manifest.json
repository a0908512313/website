{
	"manifest_version": 3,
	"name": "Translator",
	"version": "1.0",
	"description": "A simple Page Translator Extension",
	"permission": ["activeTag", "scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html",
		"default_icons": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"128": "icons/icon128.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"128": "icons/icon128.png"
	}
}
